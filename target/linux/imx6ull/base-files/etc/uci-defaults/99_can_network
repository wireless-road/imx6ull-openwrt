#!/bin/sh
#
# Copyright (C) 2013-2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case "$board" in
flexcan_ethernet)
	ip link set can0 down
	ip link set can0 up type can bitrate 500000
	ip link set can1 down
	ip link set can1 up type can bitrate 500000
	;;
esac

board_config_flush

exit 1
